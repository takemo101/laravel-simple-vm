{
    "name": "takemo101/laravel-simple-vm",
    "description": "Laravel Simple VM",
    "keywords": [
        "php",
        "package",
        "laravel",
        "view model"
    ],
    "authors": [
        {
            "name": "takemo101",
            "email": "takemo101@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/takemo101/laravel-simple-vm",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8|^9",
        "illuminate/filesystem": "^8|^9",
        "illuminate/console": "^8|^9",
        "takemo101/simple-vm": "^0.1.4"
    },
    "autoload": {
        "psr-4": {
            "Takemo101\\LaravelSimpleVM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Takemo101\\LaravelSimpleVM\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "php": "^8.0",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^1.4"
    }
}
